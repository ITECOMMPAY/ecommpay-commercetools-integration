{
	"include": ["notification", "extension", "azure"],
	"exclude": ["notification/entrypoint*.ts", "extension/entrypoint*.ts"],
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES5",
		"outDir": "dist",
		"rootDir": ".",
		"sourceMap": false,
		"strict": false,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	}
}
